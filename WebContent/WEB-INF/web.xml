       <servlet-mapping> 
             <servlet-name> movieadd</servlet-name>
             <url-pattern>/movieadd </url-pattern>
       </servlet-mapping>
       
        <servlet>
            <servlet-name>moviedelete</servlet-name>
            <servlet-class>com.movie.movieDeleteServlet</servlet-class>
       </servlet>
       
       <servlet-mapping>
            <servlet-name>moviedelete</servlet-name>
            <url-pattern>/moviedelete</url-pattern>
       </servlet-mapping>
       
       <servlet>
            <servlet-name>movieupdate</servlet-name>
