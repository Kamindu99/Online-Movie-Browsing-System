       </servlet-mapping>
       
       <servlet>
            <servlet-name>tvseriesupdate</servlet-name>
            <servlet-class>com.movie.TvSeriesUpdateServlet</servlet-class>
       </servlet>
       
       <servlet-mapping>
            <servlet-name>tvseriesupdate</servlet-name>
            <url-pattern>/tvseriesupdate</url-pattern>
       </servlet-mapping>
       
       
         <servlet>
            <servlet-name>contactusadd</servlet-name>
